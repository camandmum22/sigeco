<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://xmlns.jcp.org/jsf/html"
      xmlns:p="http://primefaces.org/ui"
      xmlns:f="http://xmlns.jcp.org/jsf/core">
    <h:head>
        <title>Proyectos aprobados decano</title>
    </h:head>
    <h:body>
        <h1 class="title">Proyectos aprobados por los decanos</h1>
        <h:form>
            <p:dataTable id="table" var="proyec" value="#{proyectosNB.darProyectosAprobadosPorDecano()}" style="margin-bottom:20px">
                <p:column headerText="Código">
                    <h:outputText  value="#{proyec.codigo}" />
                </p:column>

                <p:column headerText="Título">
                    <h:outputText  value="#{proyec.titulo}" />
                </p:column>

                <p:column headerText="Tipo de proyecto">
                    <h:outputText value="#{proyec.tipProCodigo.nombre}" />
                    
                </p:column>
                
                <p:column headerText="Presupuesto Asignado">
                    <h:outputText value="#{proyec.presupuestoAsignado}" />
                </p:column>
            </p:dataTable>
                <p:growl id="msg" showDetail="true" sticky="false" />
                <p:commandButton title="reportePDF" value="DescargarReporte" action="#{proyectosNB.generarReporteProyectosAprobadosPorDecanos()}"/>
        </h:form>
    </h:body>
</html>
